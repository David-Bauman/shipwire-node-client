{
  "name": "shipwire-node-client",
  "version": "1.0.3",
  "description": "Node.js client for the Shipwire API",
  "keywords": [
    "Shipwire",
    "API",
    "Node.js"
  ],
  "main": "dist/index.js",
  "scripts": {
    "coverage": "./node_modules/.bin/nyc report --reporter=text-lcov | ./node_modules/.bin/coveralls",
