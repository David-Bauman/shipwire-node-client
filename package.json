  },
  "license": "MIT",
  "homepage": "https://github.com/David-Bauman/shipwire-node-client",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.11.1",
    "mocha": "^5.2.0",
    "nock": "^10.0.4",
    "nyc": "^13.1.0"
  }
}
